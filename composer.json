{
  "name"             : "xervice/data-provider",
  "license"          : "MIT",
  "authors"          : [
    {
      "name" : "Mike Bertram",
      "email": "contact@mibexx.de"
    }
  ],
  "require-dev": {
    "codeception/codeception": "*",
    "symfony/var-dumper": "*",
    "phpstan/phpstan": "0.12.*-dev"
  },
  "require": {
    "php": ">=7.2.0",
    "xervice/core": "^4.0.0",
    "xervice/console": "^2.0.0",
    "symfony/finder": "^3.4|^4.0|^5.0",
    "nette/php-generator": "^3.0.0"
  },
  "autoload": {
    "psr-0": {
      "Xervice\\": "src/"
    },
    "psr-4": {
      "DataProvider\\": "src/Generated/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XerviceTest\\": "tests/Xervice/"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src/Xervice --level=7"
  }
}
